<!-- sections/slideshow2.liquid -->
{{ 'pre_flickityt4s.min.css' | asset_url | stylesheet_tag }}
{{ 'section.css' | asset_url | stylesheet_tag }}
{{ 'slideshow.css' | asset_url | stylesheet_tag }}
{{ 'slideshow2.css' | asset_url | stylesheet_tag }}
{{ 'content-position.css' | asset_url | stylesheet_tag }}
{{ 'slider-settings.css' | asset_url | stylesheet_tag }}
{%- liquid
    assign sid = section.id
    assign se_blocks = section.blocks
    assign se_stts = section.settings
    assign stt_layout = se_stts.layout
    assign se_height = se_stts.se_height
    assign nav_btn_icon = se_stts.nav_btn_icon
    assign nav_btn = se_stts.nav_btn
    assign navUI = false
    
    if stt_layout == 't4s-container-wrap' 
        assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
        assign size_img = '(min-width: 1800px) 1800px, 100vw'
    else
        assign html_layout = '__' | split: '__'
        assign size_img = '100vw'
    endif
    assign isRunLoop = true

    assign SetBL = true
    if se_height == 't4s_ratioadapt_mix'
        assign attr_img = ''
    elsif se_height == 't4s_ratioadapt_f_mix'
        assign attr_img = ''
        assign SetBL = false
    else 
        assign attr_img = 'data-'
    endif 

    if se_stts.btn_owl == "outline"
        assign arrow_icon = 1
    else
        assign arrow_icon = 2
    endif
    if nav_btn and nav_btn_icon != "default" 
        assign navUI = true
    endif 
    assign countdown = false
    assign use_button = false
    assign use_animation = false
    assign general_block = false
    assign html_content = ''
    assign html_image = ''
    assign no_image_parent = true
    assign unit_dots_vertical = se_stts.unit_dots_vertical
 -%}

{%- capture data_flickityt4s -%}
{
    "cellAlign": "center","freeScroll": 0, "contain" : 1,"dragThreshold" : 15,"percentPosition": 1,"imagesLoaded": 0,"lazyload": 0, "checkVisibility": false,
    "arrowIcon": "{{ arrow_icon }}", "t4sid": "{{ sid }}", "prevNextButtons": {{ nav_btn }},"pad": {{ se_stts.enable_slide_number_pad }},"navUI": {{ navUI }},"status": {{ se_stts.enable_slide_number }},
    "pageDots": {{ se_stts.nav_dot }},"pauseAutoPlayOnHover": {{ se_stts.au_hover }},"wrapAround": {{ se_stts.loop }},"autoPlay": {{ se_stts.au_time | times: 1000 }},
    "adaptiveHeight": {% if se_height != 't4s_ratio_fh' or se_height != 't4s_ratioadapt_f_mix' %}true{% else %}false{% endif %}
}
{%- endcapture -%}
{%- capture append_style -%}
    --aspect-ratio-cus : {{ se_stts.height_dk }}px;--aspect-ratio-custb : {{ se_stts.height_tb }}px;--aspect-ratio-cusmb : {{ se_stts.height_mb }}px;
{%- endcapture -%}

{%- if se_blocks.size > 0 -%}
   {%- for block in se_blocks %}{% assign block_type = block.type -%}
      
      {%- if block_type != 'image_parent' and no_image_parent %}{% continue -%}

      {%- elsif block_type == 'image_parent' -%}

           {%- if prev_block_image != 'nt94' and no_image_parent == false -%}

              {%- assign bk_stts = prev_block_image.settings -%}

              {%- capture content_list -%}   
                  <div class="t4s-col-item t4s-slideshow__item--content t4s-w-100 t4s-h-100 t4s-d-block" id="b_{{ prev_block_image.id }}" data-select-flickity {{ prev_block_image.shopify_attributes }}>
                      {%- comment -%} <a href="#">link content</a> {%- endcomment -%}
                        <a href="{{ bk_stts.link_item }}" target="{{ bk_stts.open_link }}" class="t4s-full-width-link{% if bk_stts.link_item == blank %} t4s-pe-none{% else %} t4s-pe-auto{% endif %}"></a>
                      
                      <div class="t4s-content-wrap t4s-pe-none t4s-pr t4s-oh t4s-w-100" style="--bg-content-wrap: {{ bk_stts.bg_content_wrap }};">
                          <div class="t4s-content-position t4s-container t4s-pa t4s-text-lg-{{ bk_stts.text_align }} t4s-text-md-{{ bk_stts.text_align_tb }} t4s-text-{{ bk_stts.text_align_mb }} t4s-bg-content-true t4s-br-content-{{ bk_stts.border_bl }} t4s-br-style-{{ bk_stts.br_style }}" style="--time-animation: {{ bk_stts.time_animation }}s;--content-pd: {{ bk_stts.content_pd_tb }}px {{ bk_stts.content_pd_lr }}px;--content-pd-tb: {{ bk_stts.content_pd_tb_tb }}px {{ bk_stts.content_pd_lr_tb }}px;--content-pd-mb: {{ bk_stts.content_pd_tb_mb }}px {{ bk_stts.content_pd_lr_mb }}px;{%- render 'content_position', ch_pos: 50, cv_pos: 50, ch_pos_tb: 50, cv_pos_tb: 50, ch_pos_mb: 50, cv_pos_mb: 50 -%};"></div>
                      </div>           
                  </div>
               {%- endcapture -%}
               {%- assign html_content = html_content | append: content_list -%}
           {%- endif -%}

           {%- liquid
            assign no_image_parent = false
            assign prev_block_image = block
            assign bk_stts = block.settings
            assign image_mb = bk_stts.image_mb
            assign image = bk_stts.image | default: image_mb
            assign mb_image = image_mb | default: image     
            assign cl_img = bk_stts.cl_img
            if forloop.first
                assign img_url_w     = image | image_url: width: 1904 | split: '1904'
                assign img_url_w0    = img_url_w[0]
                assign img_url_w1    = img_url_w[1]
                assign img_url_w_mb  = mb_image | image_url: width: 1904 | split: '1904'
                assign img_url_w_mb0 = img_url_w_mb[0]
                assign img_url_w_mb1 = img_url_w_mb[1]
                assign img_1         = img_url_w0 | append: 1 | append: img_url_w1
                assign img_1_mb      = img_url_w_mb0 | append: 1 | append: img_url_w_mb1
            else
                assign img_1    = image | image_url: width: 1
                assign img_1_mb = mb_image | image_url: width: 1
            endif
            if cl_img != blank and cl_img != 'rgba(0,0,0,0)'
                assign url_img = cl_img
                assign url_img_mb = cl_img
            else
                assign url_img = 'url(' | append: img_1 | append: ')'
                assign url_img_mb = 'url(' | append: img_1_mb | append: ')'
            endif  
            assign ani_delay = 0   
            assign percent_delay = bk_stts.animation_delay | divided_by: 100.0
            assign time_ani_delay = bk_stts.time_animation | times: percent_delay
            if isRunLoop
               assign ratio = image.aspect_ratio 
               assign ratiomb = mb_image.aspect_ratio  
               assign isRunLoop = SetBL
            endif -%}

           {%- capture image_list -%}
               <div class="t4s-col-item t4s-slideshow__item--image t4s_position_{{ bk_stts.image_position }} t4s-oh t4s-w-100 t4s-d-block" id="b_{{ block.id }}2">
                    <a href="{{ bk_stts.link_item }}" target="{{ bk_stts.open_link }}" class="t4s-full-width-link{% if bk_stts.link_item == blank %} t4s-pe-none{% else %} t4s-pe-auto{% endif %}"></a>
                   <div class="t4s_ratio t4s_ratio_hasmb" {{ attr_img }}style="--ratioapt: {{ ratio | default: 1.7777 }};--ratioaptmb: {{ ratiomb | default: 1.7777 }};">
                       {%- if image -%}   
                            {%- if forloop.first -%}
                                    <img class="t4s-img-as-bg t4s-d-md-none t4s-slide__{{ bk_stts.animate_slide }} lazyloadt4sNative t4s-lz--fadeIn"
                                    srcset="{{ img_url_w_mb0 }}375{{ img_url_w_mb1 }} 375w, {{ img_url_w_mb0 }}550{{ img_url_w_mb1 }} 550w, {{ img_url_w_mb0 }}750{{ img_url_w_mb1 }} 750w, {{ img_url_w_mb0 }}1100{{ img_url_w_mb1 }} 1100w, {{ img_url_w_mb0 }}1500{{ img_url_w_mb1 }} 1500w, {{ img_url_w_mb0 }}1780{{ img_url_w_mb1 }} 1780w, {{ img_url_w_mb0 }}2000{{ img_url_w_mb1 }} 2000w, {{ img_url_w_mb0 }}3000{{ img_url_w_mb1 }} 3000w,{{ img_url_w_mb0 }}3840{{ img_url_w_mb1 }} 3840w, {{ mb_image | image_url }} {{ mb_image.width }}w"
                                    sizes="(min-width: 768pxpx) 768px,100vw" src="{{ img_url_w_mb0 }}1500{{ img_url_w_mb1 }}"
                                    loading="lazy" onload="loadImageT4s(this)" alt="{{ mb_image.alt | escape }}" width="{{ mb_image.width }}" height="{{ mb_image.height }}">
                                    <span class="lazyloadt4s-loader is-bg-img t4s-d-md-none" style="background: {{ url_img_mb }}"></span>
                                    <img class="t4s-img-as-bg t4s-d-none t4s-d-md-block t4s-slide__{{ bk_stts.animate_slide }} lazyloadt4sNative t4s-lz--fadeIn"
                                    srcset="{{ img_url_w0 }}375{{ img_url_w1 }} 375w, {{ img_url_w0 }}550{{ img_url_w1 }} 550w, {{ img_url_w0 }}750{{ img_url_w1 }} 750w, {{ img_url_w0 }}1100{{ img_url_w1 }} 1100w, {{ img_url_w0 }}1500{{ img_url_w1 }} 1500w, {{ img_url_w0 }}1780{{ img_url_w1 }} 1780w, {{ img_url_w0 }}2000{{ img_url_w1 }} 2000w, {{ img_url_w0 }}3000{{ img_url_w1 }} 3000w,{{ img_url_w0 }}3840{{ img_url_w1 }} 3840w, {{ image | image_url }} {{ image.width }}w"
                                    sizes="{{ size_img }}" src="{{ img_url_w0 }}1500{{ img_url_w1 }}"
                                    loading="lazy" onload="loadImageT4s(this)" alt="{{ image.alt | escape }}" width="{{ image.width }}" height="{{ image.height }}">
                                    <span class="lazyloadt4s-loader is-bg-img t4s-d-none t4s-d-md-block" style="background: {{ url_img }}"></span>
                            {%- else -%}
                                <img class="lazyloadt4s t4s-lz--fadeIn t4s-img-as-bg t4s-d-md-none t4s-slide__{{ bk_stts.animate_slide }}" data-src="{{ img_1_mb }}" data-widths="[375, 575, 750]" data-optimumx="1.5" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ mb_image.width }}" height="{{ mb_image.height }}" alt="{{ mb_image.alt | escape }}">
                                <span class="lazyloadt4s-loader is-bg-img t4s-d-md-none" style="background: {{ url_img_mb }}"></span>
                                <img class="lazyloadt4s t4s-lz--fadeIn t4s-img-as-bg t4s-d-none t4s-d-md-block t4s-slide__{{ bk_stts.animate_slide }}" data-src="{{ img_1 }}" data-widths="[800, 1000, 1200, 1400, 1600, 1800, 2000, 2200, 2500, 3000, 3400, 3800, 4100]" data-optimumx="1.5" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}">
                                <span class="lazyloadt4s-loader is-bg-img t4s-d-none t4s-d-md-block" style="background: {{ url_img }}"></span>
                            {%- endif -%}
                       {%- else -%} 
                       {%- capture current -%}{% cycle 1, 2 %}{%- endcapture -%}
                           {{ 'lifestyle-' | append: current | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1' }}  
                       {%- endif -%}
                   </div>
               </div>  
           {%- endcapture -%}
           {%- assign html_image = html_image | append: image_list -%}

      {%- else -%}

           {%- liquid

           assign next_block = se_blocks[forloop.index] -%}

           {%- capture content_list -%}
               {%- if prev_block_image != 'nt94' -%}

                {%- liquid assign bk_stts = prev_block_image.settings -%}

               <div class="t4s-col-item t4s-slideshow__item--content t4s-w-100 t4s-h-100 t4s-d-block" id="b_{{ prev_block_image.id }}" data-select-flickity {{ prev_block_image.shopify_attributes }}>
                   {%- comment -%} <a href="#">link content</a> {%- endcomment -%}
                   <a href="{{ bk_stts.link_item }}" target="{{ bk_stts.open_link }}" class="t4s-full-width-link{% if bk_stts.link_item == blank %} t4s-pe-none{% else %} t4s-pe-auto{% endif %}"></a>
                   <div class="t4s-content-wrap t4s-pe-none t4s-pr t4s-oh t4s-w-100 t4s-h-100" style="--bg-content-wrap: {{ bk_stts.bg_content_wrap }};">
                       <div class="t4s-content-position t4s-container t4s-pa t4s-text-lg-{{ bk_stts.text_align }} t4s-text-md-{{ bk_stts.text_align_tb }} t4s-text-{{ bk_stts.text_align_mb }} t4s-bg-content-true t4s-br-content-{{ bk_stts.border_bl }} t4s-br-style-{{ bk_stts.br_style }}" style="--time-animation: {{ bk_stts.time_animation }}s;--content-pd: {{ bk_stts.content_pd_tb }}px {{ bk_stts.content_pd_lr }}px;--content-pd-tb: {{ bk_stts.content_pd_tb_tb }}px {{ bk_stts.content_pd_lr_tb }}px;--content-pd-mb: {{ bk_stts.content_pd_tb_mb }}px {{ bk_stts.content_pd_lr_mb }}px;{%- render 'content_position', ch_pos: 50, cv_pos: 50, ch_pos_tb: 50, cv_pos_tb: 50, ch_pos_mb: 50, cv_pos_mb: 50 -%};" >  
              {%- endif -%}
                           {%- assign bk_stts = block.settings -%} 
                           {%- case block_type -%}
                                {%- when 'custom_text' -%}
                                    {%- assign general_block = true -%}
                                    {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                                    <{{ bk_stts.tag }} data-lh="{{bk_stts.text_lh_mb}}" data-lh-md="{{bk_stts.text_lh_tb}}" data-lh-lg="{{bk_stts.text_lh}}" class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-text-bl t4s-fnt-fm-{{ bk_stts.fontf }} t4s-font-italic-{{ bk_stts.font_italic }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }} t4s-hidden-br-mb-{{ bk_stts.remove_br_tag }} t4s-text-shadow-{{ bk_stts.text_shadow }} t4s-rte--list" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'custom_text', bk_stts: bk_stts, ani_delay: ani_delay -%}>{{ bk_stts.text }}</{{ bk_stts.tag }}>
                                {%- when 'space_html' -%}
                                    {%- assign general_block = true -%}
                                    {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                                    <div class="t4s-bl-item t4s-space-html t4s-animation-{{ bk_stts.animation }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'space_html', bk_stts: bk_stts, ani_delay: ani_delay -%}></div>
                                {%- when 'html' -%}
                                    {%- assign general_block = true -%}
                                    {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                                    <div class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-raw-html t4s-rte--list t4s-hidden-mobile-{{ bk_stts.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'html', bk_stts: bk_stts, ani_delay: ani_delay -%}>{{ bk_stts.html_content }}</div>
                                {%- when 'image' -%}
                                    {%- assign image = bk_stts.image_child -%}
                                    {%- if bk_stts.img_link != blank -%}
                                    {%- assign image_parent = 'a,' | split: ',' -%}
                                    {%- else -%}
                                    {%- assign image_parent = 'div,data-' | split: ',' -%}
                                    {%- endif -%}
                                    {%- if image != blank -%}
                                        {%- assign general_block = true -%}
                                        {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                                        <{{ image_parent[0] }} {{ image_parent[1] }}href="{{ bk_stts.img_link }}" class="t4s-bl-item t4s-img-child t4s-animation-{{ bk_stts.animation }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }} t4s-pe-auto" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'image', bk_stts: bk_stts, ani_delay: ani_delay -%}>
                                            <img class="lazyloadt4s t4s-lz--fadeIn" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg', w: image.width, h: image.height %}" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}">
                                        </{{- image_parent[0] -}}>
                                    {%- else -%}
                                    {{ 'image' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1 t4s-obj-eff' }}
                                    {%- endif -%}
                                {%- when "countdown" -%}
                                    {%- if bk_stts.date != blank -%}
                                        {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                                        {%- assign countdown = true -%}
                                        <div class="t4s-bl-item t4s-countdown sepr_coun_dt_wrap t4s-countdown-des-{{ bk_stts.cdt_des }} t4s-countdown-size-{{ bk_stts.cdt_size }} t4s-animation-{{ bk_stts.animation }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'countdown', bk_stts: bk_stts, ani_delay: ani_delay -%}>
                                            <div class="t4s-time" data-countdown-t4s data-date='{{ bk_stts.date }}' data-keyid='#countdown-{{ sid }}'></div>
                                        </div>
                                    {% endif %}
                                {%- when 'custom_button' -%}
                                    {%- if bk_stts.button_link != blank and bk_stts.button_text != blank -%}
                                        {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                                        {%- assign use_button = true -%}
                                        {%- assign button_style = bk_stts.button_style -%}
                                        <a href="{{ bk_stts.button_link }}" target="{{ bk_stts.target_link }}" class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-btn t4s-btn-custom t4s-pe-auto t4s-fnt-fm-{{ bk_stts.fontf }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }} t4s-btn-style-{{ button_style }} {% if button_style == 'default' or button_style == 'outline' or button_style == 'highlights_icon' %}t4s-btn-effect-{{ bk_stts.button_effect }}{% endif %}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'custom_button', bk_stts: bk_stts, ani_delay: ani_delay -%}>{{ bk_stts.button_text }}
                                            {%- if bk_stts.button_icon_w > 0 -%}
                                                {%- if bk_stts.icon_style == "1" -%}
                                                    <svg  class="t4s-btn-icon" viewBox="0 0 14 10"><use xlink:href="#t4s-icon-btn"></use></svg>
                                                {%- else -%}
                                                    <svg class="t4s-btn-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"><path d="M96 480c-8.188 0-16.38-3.125-22.62-9.375c-12.5-12.5-12.5-32.75 0-45.25L242.8 256L73.38 86.63c-12.5-12.5-12.5-32.75 0-45.25s32.75-12.5 45.25 0l192 192c12.5 12.5 12.5 32.75 0 45.25l-192 192C112.4 476.9 104.2 480 96 480z"/></svg>   
                                                {%- endif -%}
                                            {%- endif -%}
                                        </a>
                                    {%- endif -%}
                                {%- when 'custom_text_circle' -%}
                                    {%- if bk_stts.text != blank -%}
                                        {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                                        {{ 'text-circle.css' | asset_url | stylesheet_tag }} 
                                        {%- assign image = bk_stts.image_icon -%}
                                        <div class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }} t4s-box-text-circle-wrap t4s-size-icon-{{ bk_stts.icon_size }}{% if bk_stts.use_rds %} t4s_ratio1_1 t4s_position_8 t4s_cover {%- endif -%}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'custom_text_circle', bk_stts: bk_stts, ani_delay: ani_delay -%}>
                                            <div class="t4s-box-text-circle t4s-pr t4s-oh t4s-d-inline-block">
                                            <svg viewbox="0 0 100 100" class="t4s-text-circle">
                                                <defs><path id="t4s-text-circle" d="M 50, 50 m -37, 0 a 37,37 0 1,1 74,0 a 37,37 0 1,1 -74,0"></path></defs>
                                                <text font-size="6px"><textpath xlink:href="#t4s-text-circle">{{ bk_stts.text | strip_html }}</textpath></text>
                                            </svg>
                                            {%- if bk_stts.source == 'themes_icon' -%}
                                                {%- if bk_stts.icon_themes != blank and bk_stts.icon_themes != "none" -%}
                                                <div class="t4s-img-icon t4s-theme-icon">
                                                    {%- render 'icon_shipping', icon_name: bk_stts.icon_themes %}
                                                </div>
                                                {%- endif -%}
                                            {%- elsif bk_stts.source == 'get_image' -%}
                                                {%- if image != blank -%}
                                                <div class="t4s-img-icon t4s-img-icon {% if bk_stts.use_rds %}t4s_ratio{%- endif -%}">
                                                    <img class="lazyloadt4s" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg', w: image.width, h: image.height %}" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}">
                                                </div>
                                                {%- endif  -%}
                                            {%- else -%}
                                                {%- if bk_stts.icon != blank -%}
                                                <div class="t4s-img-icon t4s-las-icon">
                                                    <i class="{{ bk_stts.icon }}"></i>
                                                </div>
                                                {%- endif  -%}
                                            {%- endif  -%}
                                            </div>
                                        </div>
                                    {%- endif -%}
                                {%- when 'pr_simple' -%}
                                    {%- assign product = bk_stts.product -%}
                                    {%- if product != blank -%}
                                        {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
