.finetune-data/
.finetune-output/
.finetune-configs/

# Cursor debug logs
.cursor/debug*.log

# Supabase temp files (local dev state)
supabase/.temp/

# Verification data (ephemeral analysis artifacts)
.verification/

# Test benchmark results (generated per-run)
tests/integration/results/
