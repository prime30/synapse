# dependencies
/node_modules
/.pnp
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/versions

# testing
/coverage

# next.js
/.next/
/out/

# production
/dist

# electron desktop builds
/electron/dist/
/electron/node_modules/
/build/icons/

# keep the source SVG icon
!build/icon.svg

# misc
.DS_Store
*.pem

# debug
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# env files (can opt-in for committing if needed)
.env*
!.env.example
!.env.test.example

# vercel
.vercel

# typescript
*.tsbuildinfo
next-env.d.ts

# dev-only tools (not shipped)
components/marketing/dev/

# Local theme file sync (dev-only mirror of Supabase files)
.synapse-themes/

# Local file cache (theme files cached on disk for zero-latency agent reads)
.cache/

