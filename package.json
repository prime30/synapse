    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.182.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/coverage-v8": "^4.0.18",
    "babel-plugin-react-compiler": "1.0.0",
    "chokidar": "^5.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "dotenv": "^16.4.5",
    "electron": "^40.6.0",
    "electron-builder": "^26.8.1",
    "electron-is-dev": "^3.0.1",
    "electron-notarize": "^1.2.2",
    "electron-serve": "^3.0.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "jsdom": "^28.0.0",
    "node-pty": "^1.1.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite-tsconfig-paths": "^6.1.0",
    "vitest": "^4.0.18",
    "wait-on": "^9.0.4"
  }
}
