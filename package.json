{
  "name": "synapse1",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000 --webpack",
    "dev:webpack": "next dev -p 3000 --webpack",
    "dev:electron": "concurrently \"cross-env NEXT_PUBLIC_ENABLE_LOCAL_SYNC=1 npm run dev\" \"wait-on tcp:3000 && electron electron/main.js\"",
    "dev:desktop": "concurrently \"npm run dev\" \"cd electron && npm run start:dev\"",
    "build": "next build --webpack",
    "build:desktop": "npm run build && npm run icons && cd electron && npx electron-builder",
    "build:desktop:win": "npm run build && npm run icons && cd electron && npx electron-builder --win",
    "build:desktop:mac": "npm run build && npm run icons && cd electron && npx electron-builder --mac",
    "build:desktop:linux": "npm run build && npm run icons && cd electron && npx electron-builder --linux",
    "start": "next start",
    "electron": "electron electron/main.js",
    "icons": "npx tsx scripts/generate-icons.ts",
    "lint": "eslint && npm run lint:no-emoji && npm run lint:migrations",
    "lint:no-emoji": "node scripts/check-no-emoji.mjs",
    "lint:migrations": "node scripts/check-supabase-migration-versions.mjs",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:live-agents": "vitest run tests/integration/agents-e2e-live.test.ts",
    "db:push": "npx supabase db push",
    "generate-marketing-assets": "npx tsx scripts/generate-marketing-assets.ts",
    "benchmark:use-latest": "npx tsx scripts/sync-benchmark-results.ts",
    "benchmark:cursor-only": "vitest run tests/integration/v2-live-benchmark.test.ts -t \"Cursor only\"",
    "cursor:timing": "npx tsx scripts/run-cursor-agent-timing.ts",
    "cursor:index": "npx tsx scripts/cursor-index-theme.ts",
    "synapse:harness": "npx tsx scripts/run-synapse-agent-harness.ts",
    "synapse:harness:arch": "npx tsx scripts/run-synapse-agent-harness.ts --suite architecture",
    "synapse:harness:cx": "npx tsx scripts/run-synapse-agent-harness.ts --suite prompt-first",
    "synapse:harness:cx:ci": "npx tsx scripts/run-synapse-agent-harness.ts --suite prompt-first --mode v2 --json --json-file .verification/synapse-cx-summary.json --fail-on-threshold --min-routing-accuracy 1 --min-behavior-accuracy 1 --min-completion-coverage 0",
    "finetune:dataset": "npx tsx scripts/build-training-dataset.ts",
    "finetune:config": "npx tsx scripts/generate-llamafactory-config.ts",
    "finetune:eval": "npx tsx scripts/run-finetune-eval.ts --mode v2",
    "eval:bugs": "npx tsx scripts/run-finetune-eval.ts --mode bugs",
    "eval:e2e": "npx tsx scripts/run-e2e-eval.ts",
    "finetune:eval:ci": "npx tsx scripts/run-finetune-eval.ts --mode v2 --json-file .verification/finetune-eval-summary.json --fail-on-regression",
    "finetune:weekly": "npx tsx scripts/run-weekly-learning-cycle.ts --json-file .verification/weekly-cycle-report.json",
    "finetune:baseline": "npx tsx scripts/run-weekly-learning-cycle.ts --capture-baseline --json-file .verification/weekly-cycle-report.json",
    "bump:patch": "node scripts/bump-version.mjs patch",
    "bump:minor": "node scripts/bump-version.mjs minor",
    "bump:major": "node scripts/bump-version.mjs major"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^16.1.6",
    "@floating-ui/dom": "^1.7.5",
    "@google/genai": "^1.41.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@monaco-editor/react": "^4.7.0",
    "@react-three/fiber": "^9.5.0",
    "@sparticuz/chromium-min": "^143.0.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.95.3",
    "@tanstack/react-query": "^5.90.20",
    "@types/picomatch": "^4.0.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@upstash/redis": "^1.36.2",
    "@xyflow/react": "^12.10.0",
    "clsx": "^2.1.1",
    "contentful": "^11.10.3",
    "dagre": "^0.8.5",
    "diff": "^8.0.3",
    "framer-motion": "^12.33.0",
