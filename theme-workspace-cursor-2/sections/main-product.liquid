                  assign class_gx_lg = ' t4s-g-lg-10 '
                endif
              -%}
              {{ 'slider-settings.css' | asset_url | stylesheet_tag }}

              <div class="slick_gallery t4s-product__info-container--sticky" style="display:none;">
